{
  "version": 2,
  "routes": [
    { "src": "/(privacy|terms)\\.html", "headers": { "Cache-Control": "public, max-age=3600" } },
    { "src": "/(robots\\.txt|sitemap\\.xml)", "headers": { "Cache-Control": "public, max-age=3600" } },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "redirects": [
    { "source": "/(.*)", "destination": "https://amiraq.org/$1", "permanent": false }
  ]
}
